public type ContextGreeting record {|
    Greeting content;
    map<string|string[]> headers;
|};

public type ContextString record {|
    string content;
    map<string|string[]> headers;
|};

public type ContextTimestamp record {|
    time:Utc content;
    map<string|string[]> headers;
|};

public type Greeting record {|
    string name = "";
    time:Utc time = [0, 0.0d];
|};

