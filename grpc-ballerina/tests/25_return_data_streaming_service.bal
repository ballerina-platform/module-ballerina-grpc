import ballerina/io;

listener Listener ep25 = new (9115);

@ServiceDescriptor {
    descriptor: ROOT_DESCRIPTOR_25,
    descMap: getDescriptorMap25()
}
service "HelloWorld25" on ep25 {

    remote isolated function lotsOfReplies(string name) returns stream<string> {
        io:println("Server received hello from " + name);
        string[] greets = [("Hi " + name), ("Hey " + name), ("GM " + name)];
        return greets.toStream();
    }
}

public type ContextStringStream record {|
    stream<string> content;
    map<string|string[]> headers;
|};

const string ROOT_DESCRIPTOR_25 = "";
isolated function getDescriptorMap25() returns map<string> {
    return {
        "25_return_data_streaming.proto":"",
        "google/protobuf/wrappers.proto":""

    };
}

